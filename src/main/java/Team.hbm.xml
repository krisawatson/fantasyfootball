<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.fantasyfootball.ultimate.model">
 
    <class name="Team" table="team">
    	<id name="id" type="int" column="id">
            <generator class="assigned"/>
        </id>
        <property name="code" />
        <property name="shortName" column="short_name" />
        <property name="unavailable" />
        <property name="strength" />
        <property name="teamDivision" column="team_division"/>
        <property name="position" />
        <property name="played" />
        <property name="win" />
        <property name="lose" />
        <property name="draw" />
        <property name="points" />
        <property name="form" />
        <many-to-one name="currentEventFixtureID" class="com.fantasyfootball.ultimate.model.EventFixture" fetch="select">
            <column name="current_event_fixture_id" not-null="true" />
        </many-to-one>
        <many-to-one name="nextEventFixtureID" class="com.fantasyfootball.ultimate.model.EventFixture" fetch="select">
            <column name="next_event_fixture_id" not-null="true" />
        </many-to-one>
    </class>
 
</hibernate-mapping>